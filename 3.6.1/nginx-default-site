#
upstream app {
	server unix:///tmp/uwsgi.sock;
}

#
server {
	listen 8080 default_server;
	listen [::]:8080 default_server;

	root /home/site/wwwroot;
	index index.html index.htm index.nginx-debian.html;

	server_name _;
	charset utf-8;
	
	location /images {
	}

	location /css {
	}

	location /js {
	}

	location /static {
	}

        location / {
                uwsgi_pass app;
                include /etc/nginx/uwsgi_params;
        }
}


